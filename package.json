{
  "name": "fullmag-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Multi-platform e-commerce system",
  "scripts": {
    "dev:web": "pnpm --filter @fullmag/web dev",
    "dev:api": "pnpm --filter @fullmag/api dev",
    "dev-watch:api": "pnpm --filter @fullmag/api start:dev",
    "dev:mobile": "pnpm --filter @fullmag/mobile start",
    "dev:mobile:android": "pnpm --filter @fullmag/mobile android",
    "dev:mobile:ios": "pnpm --filter @fullmag/mobile ios",
    "dev:seed": "pnpm --filter @fullmag/api seed",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean",
    "install:whatsapp": "pnpm --filter @fullmag/whatsapp install",
    "dev:whatsapp": "pnpm --filter @fullmag/whatsapp start:dev",
    "build:whatsapp": "pnpm --filter @fullmag/whatsapp build",
    "start:whatsapp": "pnpm --filter @fullmag/whatsapp start"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "typescript": "5.3.3",
    "prettier": "^3.2.4",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b"
}
